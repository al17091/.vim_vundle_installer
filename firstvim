if 0| endif

filetype off

if has('vim_starting')
	if &compatible
		set nocompatible
	endif
	set rtp+=~/.vim/bundle/neobundle.vim
endif

call neobundle#begin(expand('~.vim/bundle/'))
NeoBundle 'Shougo/neobundle.vim'

call neobundle#end()

filetype plugin indent on
filetype indent on
syntax on
NeoBundleCheck

